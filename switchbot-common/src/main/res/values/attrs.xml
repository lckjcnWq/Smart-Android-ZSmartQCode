      <flag name="left" value="0x03" />
      <flag name="right" value="0x05" />
      <flag name="center_horizontal" value="0x01" />
    </attr>
    <!-- 是否开启自动轮播 -->
    <attr format="boolean" name="mybanner_pointAutoPlayAble" />
    <!-- 自动轮播的时间间隔 -->
    <attr format="integer" name="mybanner_pointAutoPlayInterval" />
    <!-- 页码切换过程的时间长度 -->
    <attr format="integer" name="mybanner_pageChangeDuration" />
    <!-- 页面切换的动画效果 -->
    <attr format="enum" name="mybanner_transitionEffect">
      <enum name="defaultEffect" value="0" />
      <enum name="alpha" value="1" />
      <enum name="rotate" value="2" />
      <enum name="cube" value="3" />
      <enum name="flip" value="4" />
      <enum name="accordion" value="5" />
