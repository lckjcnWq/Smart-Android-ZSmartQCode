      <enum name="stack" value="10" />
      <enum name="depth" value="11" />
      <enum name="zoom" value="12" />
    </attr>
    <!-- 提示文案的文字颜色 -->
    <attr format="reference|color" name="mybanner_tipTextColor" />
    <!-- 提示文案的文字大小 -->
    <attr format="dimension" name="mybanner_tipTextSize" />
    <!-- 加载网络数据时覆盖在 BGABanner 最上层的占位图 -->
    <attr format="reference" name="mybanner_placeholderDrawable" />
    <!-- 是否是数字指示器 -->
    <attr format="boolean" name="mybanner_isNumberIndicator" />
    <!-- 数字指示器文字颜色 -->
    <attr format="reference|color" name="mybanner_numberIndicatorTextColor" />
    <!-- 数字指示器文字大小 -->
    <attr format="dimension" name="mybanner_numberIndicatorTextSize" />
    <!-- 数字指示器背景 -->
    <attr format="reference" name="mybanner_numberIndicatorBackground" />
    <!-- 当只有一页数据时是否显示指示器，默认值为 false -->
    <attr format="boolean" name="mybanner_isNeedShowIndicatorOnOnlyOnePage" />
    <!-- 自动轮播区域距离 BGABanner 底部的距离 -->
    <attr format="dimension" name="mybanner_contentBottomMargin" />
    <!-- 宽高比例，如果大于 0，则会根据宽度来计算高度，否则使用 android:layout_height 指定的高度 -->
    <attr format="float" name="mybanner_aspectRatio" />
    <!-- 占位图和资源图片缩放模式 -->
